{% if attributes.toArray() is not empty %}
<div{{ attributes.addClass('reset-filters') }}>
  {% endif %}
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {{ content }}
  {% endblock %}
  {% if attributes.toArray() is not empty %}
</div>
{% endif %}
