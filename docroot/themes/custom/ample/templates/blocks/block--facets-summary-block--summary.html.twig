{% if attributes.toArray() is not empty %}
<div{{ attributes }}>
  {% endif %}
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    <div class="reset-filters">
      {{ content }}
    </div>
  {% endblock %}
  {% if attributes.toArray() is not empty %}
</div>
{% endif %}
