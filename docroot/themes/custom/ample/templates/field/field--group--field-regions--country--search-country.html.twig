{% for item in items %}
  {% set region = item.content|render %}
  {% if region is not empty %}
    <div{{ item.attributes.addClass('regoin-wrapper') }}>
      <div class="big iconwrapper">
        <div class="p4hicon">j</div>
      </div>
      <div class="blue largetxt left">
        {{ "Part of the @region Region"|t({ '@region': region }) }}
      </div>
    </div>
  {% endif %}
{% endfor %}
