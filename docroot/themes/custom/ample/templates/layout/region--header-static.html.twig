  </div>
  {% spaceless %}
    <div class="bottom-head {{ elements.countrymenublock ? 'has-anchor-links' : '' }}">
      {% if elements.countrymenublock %}
        {{ elements.countrymenublock }}
      {% endif %}
    </div>
  {% endspaceless %}
