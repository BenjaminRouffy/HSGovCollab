{% if title %}
  <div class="title-wrapper" data-anchor-id="view-{{ dom_id }}">
    <h1 role="heading" class="anchor-title">
      {{ title }}
    </h1>

    <div class="line"></div>
  </div>
{% endif %}

<div class="article-list">
  {% for row in rows %}
    {{ row.content }}
  {% endfor %}
</div>
