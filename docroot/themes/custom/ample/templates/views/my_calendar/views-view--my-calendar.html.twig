{% set classes = [dom_id ? 'js-view-dom-id-' ~ dom_id] %}

{% if without_wrapper is not defined %}
<div{{ attributes.addClass(classes) }}>
  {% endif %}
  <div class="top-text-region big-title">
    <div class="title-wrapper">
      {{ title_prefix }}
      <h1 role="heading" class="anchor-title">
        {{ view.getTitle()|raw }}
      </h1>
      {{ title_suffix|without('contextual_links') }}
      <div class="line"></div>
    </div>
  </div>

  {% if header %}
    {% if header_without_wrapper is not defined %}
    <{{ header_tag|default('h2') }}{{ header_classes }}>
      {{ header }}
    </{{ header_tag|default('h2') }}>
    {% else %}
    {{ header }}
    {% endif %}
  {% endif %}

  {{ exposed }}
  {{ attachment_before }}

  {{ pager }}

  {{ rows }}

  {% if empty %}
    <div class="empty-result">
      {{ empty }}
    </div>
  {% endif %}
  {{ pager }}

  {{ attachment_after }}

<div class="bottom-link">
  {{ more }}
  {{ feed_icons }}
</div>

{% if footer %}
  {% if footer_without_wrapper is not defined %}
    <{{ footer_tag|default('div') }}{{ footer_classes }}>
    {{ footer }}
    </{{ footer_tag|default('div') }}>
  {% else %}
    {{ footer }}
  {% endif %}
{% endif %}

  <div id="calendar"></div>
  <div id="event-response"></div>
{% if without_wrapper is not defined %}
  </div>
{% endif %}
